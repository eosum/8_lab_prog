<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<AnchorPane fx:id="body" prefHeight="560" prefWidth="766" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main_package.controllers.HelpController">
    <children>
        <ComboBox fx:id="languageChoose"/>
        <Label fx:id="realHeroLabel" text="%realHeroHelp" />
        <Label fx:id="hasToothpickLabel" text="%toothpickHelp" />
        <Label fx:id="weaponTypeLabel" text="%weaponTypeHelp" />
        <Label fx:id="xLabel" text="%xHelp" />
        <Label fx:id="yLabel" text="%yHelp" />
        <Label fx:id="infoLabel" text="%infoHelp" />

        <Button fx:id="backButton" mnemonicParsing="false" text="%goBackButton" />
    </children>
</AnchorPane>
